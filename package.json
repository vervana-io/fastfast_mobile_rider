{
  "name": "FastFastRider",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "yarn runOnSimulator15",
    "lint": "eslint .",
    "start": "react-native start",
    "start:dev": "react-native start --experimental-debugger",
    "test": "jest",
    "android:clean": "cd android && ./gradlew clean",
    "pod-install": "cd ios && pod install",
    "pod-update": "cd ios && pod repo update",
    "build": "react-native run-android --variant=release",
    "bundleRelease": "cd android && ./gradlew clean && ./gradlew bundleRelease",
    "buildScript": "cd android && ./gradlew clean && ./gradlew assembleRelease && ./gradlew bundleRelease",
    "android:bundle:release": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/",
    "refreshBuild": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "patch": "npx patch-package react-native-inappbrowser-reborn --use-yarn",
    "availableIOSSimulator": "xcrun simctl list devices",
    "runOnSimulator15": "npx react-native run-ios --simulator='iPhone 16 Pro'"
  },
